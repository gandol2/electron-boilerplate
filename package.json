{
  "name": "{{PROJECT_NAME}}",
  "version": "0.1.0",
  "description": "{{PROJECT_DESCRIPTION}}",
  "productName": "{{PRODUCT_NAME}}",
  "author": "{{AUTHOR_NAME}}",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:vite\" \"pnpm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:{{DEV_PORT}} && node scripts/build-main.js && cross-env VITE_DEV_SERVER_URL=http://localhost:{{DEV_PORT}} electron .",
    "postinstall": "electron-rebuild -f -w keytar",
    "rebuild": "electron-rebuild -f -w keytar",
    "build": "pnpm run build:renderer && pnpm run build:main",
    "build:renderer": "vite build",
    "build:main": "node scripts/build-main.js",
    "predist": "node -e \"try{require('fs').rmSync('dist-electron',{recursive:true,force:true})}catch(e){console.log('Clean skipped:',e.message)}\"",
    "dist": "pnpm run build && electron-builder",
    "publish:app": "node scripts/publish.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "electron-updater": "^6.7.3",
    "embla-carousel-react": "^8.6.0",
    "keytar": "^7.9.0",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.27.2",
    "eslint": "^8.56.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.16",
    "wait-on": "^7.2.0"
  }
}
